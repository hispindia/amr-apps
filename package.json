{
    "name": "amr-apps",
    "version": "0.2.2",
    "description": "AMR apps for DHIS2",
    "main": "index.js",
    "author": "Terje Uglebakken <terjeug@ifi.uio.no>",
    "license": "BSD-3-Clause",
    "private": true,
    "scripts": {
        "analyze:approval": "source-map-explorer 'build/approval/static/js/*.js'",
        "analyze:entry": "source-map-explorer 'build/entry/static/js/*.js'",
        "bump": "node scripts/bump.js",
        "build": "NODE_ENV=production yarn clean && yarn build:shared && yarn build:approval && yarn build:entry",
        "build:approval": "node scripts/approval.js",
        "build:entry": "node scripts/entry.js",
        "build:shared": "BABEL_ENV=cjs babel src --out-dir build --copy-files --verbose",
        "clean": "rimraf build",
        "clean:node": "rimraf node_modules",
        "format": "d2-style js apply",
        "launch": "./scripts/konsole/launch.sh",
        "postbuild:approval": "rimraf amr-approval.zip && cd build/approval && bestzip ../../amr-approval.zip *",
        "postbuild:entry": "rimraf amr-entry.zip && cd build/entry && bestzip ../../amr-entry.zip *",
        "prebuild:approval": "d2-manifest approval/manifest.json approval/public/manifest.webapp",
        "prebuild:entry": "d2-manifest entry/manifest.json entry/public/manifest.webapp",
        "start": "node scripts/watch.js"
    },
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.4.0",
        "@babel/plugin-proposal-class-properties": "^7.4.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
        "@babel/plugin-transform-react-constant-elements": "^7.2.0",
        "@babel/preset-env": "^7.4.2",
        "@babel/preset-react": "^7.0.0",
        "@dhis2/cli-style": "^4.1.1",
        "bestzip": "^2.1.4",
        "chokidar": "^2.0.4",
        "d2-manifest": "^1.0.0",
        "fs-extra": "^7.0.0",
        "moment": "^2.24.0",
        "react-dev-utils": "^8.0.0",
        "react-scripts": "^3.0.1",
        "redux-logger": "^3.0.6",
        "rimraf": "^2.6.3",
        "source-map-explorer": "^1.8.0"
    },
    "dependencies": {
        "@babel/runtime": "^7.3.1",
        "@date-io/dayjs": "^1.3.5",
        "@dhis2/app-runtime": "^1.3.0",
        "@dhis2/ui-core": "3.6.3",
        "@dhis2/ui-widgets": "^1.0.8",
        "@material-ui/core": "^3.9.3",
        "@material-ui/icons": "^3.0.2",
        "dayjs": "^1.8.14",
        "material-ui-pickers": "^2.2.4",
        "mui-datatables": "^2.0.0-beta.59",
        "prop-types": "^15.7.2",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-redux": "^7.1.0",
        "react-router-dom": "^5.0.0",
        "redux": "^4.0.1",
        "redux-thunk": "^2.3.0",
        "styled-components": "^5.0.0-beta.6",
        "typeface-roboto": "^0.0.54"
    }
}