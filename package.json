{
  "name": "@amr/lib",
  "version": "0.9.0",
  "description": "AMR Approval and Capture app for DHIS2",
  "main": "index.js",
  "author": "Terje Uglebakken <terjeug@ifi.uio.no>",
  "license": "BSD-3-Clause",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production yarn build:approval && yarn build:capture",
    "build:approval": "node scripts/approval.js",
    "build:capture": "node scripts/capture.js",
    "clean": "rm -rf ./build/*",
    "clean:node": "rm -rf ./node_modules/*",
    "format": "packages code-style",
    "postbuild:approval": "cp approval/manifest.webapp build/approval/ && yarn zip:approval",
    "postbuild:capture": "cp capture/manifest.webapp build/capture/ && yarn zip:capture",
    "release": "packages release",
    "start": "node scripts/watch.js",
    "zip:approval": "cd build/approval && zip -r -X ../../amr-record-approval.zip .",
    "zip:capture": "cd build/capture && zip -r -X ../../amr-record-capture.zip ."
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-react-constant-elements": "^7.2.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@dhis2/packages": "^1.3.0",
    "babel-plugin-transform-imports": "^1.5.1",
    "chokidar": "^2.0.4",
    "fs-extra": "^7.0.0",
    "husky": "^1.0.1",
    "react-dev-utils": "^8.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "@date-io/moment": "^1.0.1",
    "@dhis2/ui": "^1.0.0-beta.12",
    "@material-ui/core": "^3.9.0",
    "@material-ui/icons": "^3.0.2",
    "d2-manifest": "^1.0.0",
    "material-ui-pickers": "^2.1.1",
    "moment": "^2.23.0",
    "mui-datatables": "^2.0.0-beta.56",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^2.1.8",
    "styled-components": "^4.1.3",
    "typeface-roboto": "0.0.54"
  }
}
